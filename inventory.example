# Harbor Ansible Inventory Example
# Copy this file to 'inventory' and customize it for your environment

[harbor]
# Example: Single Harbor server
harbor1 ansible_host=192.168.1.100 ansible_user=root

# Example: Multiple Harbor servers
# harbor2 ansible_host=192.168.1.101 ansible_user=root ansible_password="your-password"
# harbor3 ansible_host=192.168.1.102 ansible_user=root

[harbor:vars]
# Common SSH settings (optional)
# ansible_ssh_common_args='-o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null'
# ansible_ssh_extra_args='-o ConnectTimeout=30'
